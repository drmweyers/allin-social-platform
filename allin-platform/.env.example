# Node Environment
NODE_ENV=development

# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/allin?schema=public"

# Redis
REDIS_URL="redis://localhost:6380"

# API Configuration
API_PORT=5000
FRONTEND_URL=http://localhost:3000

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-here-min-32-chars

# JWT
JWT_SECRET=your-jwt-secret-here-min-32-chars
JWT_REFRESH_SECRET=your-jwt-refresh-secret-here-min-32-chars

# Email Service (Development)
EMAIL_SERVICE=mailhog
MAILHOG_HOST=localhost
MAILHOG_PORT=1025

# Email Service (Production - Mailgun)
# MAILGUN_API_KEY=your-mailgun-api-key
# MAILGUN_DOMAIN=mg.yourdomain.com
# MAILGUN_EU=false
# MAILGUN_FROM_EMAIL=noreply@yourdomain.com

# Social Media APIs (Add your keys)
# TWITTER_CLIENT_ID=
# TWITTER_CLIENT_SECRET=
# FACEBOOK_APP_ID=
# FACEBOOK_APP_SECRET=
# LINKEDIN_CLIENT_ID=
# LINKEDIN_CLIENT_SECRET=
# INSTAGRAM_CLIENT_ID=
# INSTAGRAM_CLIENT_SECRET=

# OpenAI
# OPENAI_API_KEY=your-openai-api-key
# OPENAI_ORGANIZATION=your-org-id

# AWS S3 (Optional)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=us-east-1
# AWS_S3_BUCKET=

# Stripe (Optional)
# STRIPE_SECRET_KEY=
# STRIPE_PUBLISHABLE_KEY=
# STRIPE_WEBHOOK_SECRET=

# Sentry (Optional)
# SENTRY_DSN=

# Feature Flags
ENABLE_AI_FEATURES=true
ENABLE_MCP=true
ENABLE_AGENTS=true